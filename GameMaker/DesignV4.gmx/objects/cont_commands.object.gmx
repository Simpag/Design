    //Deactivate
    active = false;
    command = ""; //Clear command
    global.freezeGame = false;
}

if (keyboard_check_pressed(vk_enter) &amp;&amp; active) { 
    //Deactivate
    active = false;
    command = keyboard_string;
    scr_check_command();
    global.freezeGame = false;    
}
