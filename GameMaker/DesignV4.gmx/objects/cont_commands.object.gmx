            <string>//Clear keyboard string if it isnt active
if (!active) {
    keyboard_string = "";
}

if (keyboard_check(vk_control) &amp;&amp; keyboard_check_pressed(vk_f5)) {
    if (!active) {
        //Activate console
        active = true;
        global.freezeGame = true;
        boxX = 0;
    } else {
        //Deactivate
        active = false;
        command = ""; //Clear command
        global.freezeGame = false;
    }
}

