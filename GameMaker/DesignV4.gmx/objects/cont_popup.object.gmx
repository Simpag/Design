            case 2:
                 y +=  0.02 * power(float,2) + 0.5;
                 float += floatSpeed;
                 
                 //Destroy the popup
                 if (y - startY &gt; 150) instance_destroy();
             break;
         //Down
             case 3:
             case 4:
                 y -=  0.02 * power(float,2) + 0.5;
                 float += floatSpeed;
                 
                 //Destroy the popup
                 if (startY - y &gt; 150) instance_destroy();
             break;
        }
        
//Wrong command
    case "wrongCommand":
        y -=  0.02 * power(float,2) + 0.5;
        float += floatSpeed;
        
        //Destroy the popup
        if (startY - y &gt; 150) instance_destroy();
    break;
}
</string>
